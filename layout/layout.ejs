        <div id="optionsMenu"class="MenuDiv">
            <div id="share" class="menuText">
                <a href="/" class="post-url">分享本页</a>
            </div>

        </div></div>
    <div id="about" class="FinderText">&nbsp关于&nbsp
        <div id="aboutMenu"class="MenuDiv">

            <div id="aboutMain" class="menuText">
                <a href="/" class="post-url">个人简介</a>
            </div>
            <div  class="menuText">
                <a href="/" class="post-url">友情链接</a>
            </div>

            <div  class="menuText">
                <a href="makinoharashoko@outlook.com" class="post-url">邮箱</a>
            </div>
            <div  class="menuText">
                <a href="https://github.com/MakinoharaShoko" class="post-url">Github</a>
            </div>
            <div id="twitter"  class="menuText">
                <a href="https://twitter.com/_Makinohara" class="post-url">Twitter</a>
            </div>

            <div  class="menuText">
                <a href="https://github.com/MakinoharaShoko/hexo-OSX-theme" class="post-url">Theme OS-X by Makinohara</a>
            </div>
            <div id="author" class="menuText">
                <a href="/" class="post-url">©2020 MSFASR Centre Telecom</a>
            </div>
        </div><nbsp></nbsp></div>
    <img src="/img/iconsTop.png" alt="icons"id="icons"/>
    <div id="showtime"></div>

</div>

<!--now write body-->
<div id="body">
    <%- body %>
</div>

<!--Now Write Bottom -->
<div id="Bottom">
    <img src="/img/mail.png" class="BottomImg">
    <img src="/img/missioncontrol.png" class="BottomImg">
    <img src="/img/reminders.png" class="BottomImg">
    <img src="/img/preview.png" class="BottomImg">
</div>



<%- js('js/osx.js') %>
